{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "cli": {
    "packageManager": "npm"
  },
  "newProjectRoot": "projects",
  "projects": {
    "event-monitoring-components": {
      "projectType": "library",
      "root": "projects/event-monitoring-components",
      "sourceRoot": "projects/event-monitoring-components/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "configurations": {
            "production": {
              "tsConfig": "projects/event-monitoring-components/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/event-monitoring-components/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:unit-test",
          "options": {
            "tsConfig": "projects/event-monitoring-components/tsconfig.spec.json"
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "configDir": ".storybook",
            "browserTarget": "event-monitoring-components:build",
            "compodoc": false,
            "port": 6006
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "configDir": ".storybook",
            "browserTarget": "event-monitoring-components:build",
            "compodoc": false,
            "outputDir": "dist/storybook"
          }
        }
      }
    }
  }
}
