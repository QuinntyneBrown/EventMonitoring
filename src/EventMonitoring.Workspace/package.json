{
  "name": "event-monitoring.workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:lib": "ng build event-monitoring-components",
    "build:lib:watch": "ng build event-monitoring-components --watch",
    "test:lib": "vitest run --project event-monitoring-components",
    "test:lib:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "storybook": "ng run event-monitoring-components:storybook",
    "build-storybook": "ng run event-monitoring-components:build-storybook"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.1.0",
  "dependencies": {
    "@angular/common": "^21.0.0",
    "@angular/compiler": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/forms": "^21.0.0",
    "@angular/platform-browser": "^21.0.0",
    "@angular/platform-browser-dynamic": "^21.0.8",
    "@angular/router": "^21.0.0",
    "chart.js": "^4.4.0",
    "gridstack": "^12.4.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "^0.16.0"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^2.2.1",
    "@angular-devkit/build-angular": "^21.0.0",
    "@angular/build": "^21.0.5",
    "@angular/cli": "^21.0.5",
    "@angular/compiler-cli": "^21.0.0",
    "@playwright/test": "^1.57.0",
    "@storybook/addon-a11y": "^8.6.15",
    "@storybook/addon-essentials": "^8.6.15",
    "@storybook/addon-interactions": "^8.6.15",
    "@storybook/angular": "^8.6.15",
    "@storybook/test": "^8.6.15",
    "@vitest/coverage-v8": "^4.0.0",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.0.0",
    "storybook": "^8.6.15",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  }
}
